{
    "version":2,
    "builds":[
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config":{
                "runtime":"python3.9"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "hooks": {
      "build": "sh build.sh"
    }
}